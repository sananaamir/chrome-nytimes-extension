{
  "manifest_version": 2,

  "name": "QuikNYT",
  "description": "Chrome extension that lets you read snippets of about 10 articles from nytimes",
  "version": "1.2.0",

  "browser_action": {
    "default_icon": "quiknyt.png",
    "default_popup": "popup.html"
  },
    "icons": {
        "19": "quiknyt.png",
        "48": "quiknyt48.png",
        "128": "quiknyt.png"
    }
}